        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="Delete old results.csv file" enabled="true">
          <stringProp name="TestPlan.comments">/**
*
* This simple script will clear contents of the results.csv file before each run.
* It comes handy especially when running in GUI mode.
*
* More info: http://stackoverflow.com/a/20591502
*
**/</stringProp>
          <stringProp name="BeanShellSampler.query">String projectHome = vars.get(&quot;PROJECT_HOME&quot;);
File file = new File(projectHome+&quot;/results/results.csv&quot;); 

if (file.exists()) {
	PrintWriter pw = new PrintWriter(file.getAbsolutePath());
	pw.close();
} else {
	log.info(&quot;File doesn&apos;t exists: &quot; + file.getAbsolutePath());
}</stringProp>
          <stringProp name="BeanShellSampler.filename"></stringProp>
